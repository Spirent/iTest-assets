        <item name="getScreenshot" isPublic="true" isVBlock="true">
            <responseMap>project://d_ADB/response_maps/JsonBlockToQueries.ffrm</responseMap>
            <steps>
                <item guid="92a61d79-a849-405b-bf6c-55485c2909b7" action="eval">
