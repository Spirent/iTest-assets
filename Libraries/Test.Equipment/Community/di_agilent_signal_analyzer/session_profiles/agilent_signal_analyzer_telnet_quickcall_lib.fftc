        <item name="SetTraceDetectorNormal" isPublic="true" defaultSessionProfile="project://di_signal_analyzer/session_profiles/signal_analyzer_telnet.ffsp">
            <steps>
                <item guid="81ed0f05-cf2c-4159-a347-37f823f0f6fe" action="command" session="$session" normalOffset="9.239" estimatedStepExecutionTime="0.003">
