        <item name="SetTraceDisplayState" isPublic="true" defaultSessionProfile="project://di_signal_analyzer/session_profiles/signal_analyzer_telnet.ffsp">
            <steps>
                <item guid="89986d9f-7c7b-4d61-afa5-fe37863b7efb" action="command" session="$session" normalOffset="9.239" estimatedStepExecutionTime="0.003">
