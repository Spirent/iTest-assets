        <item name="SetMarkerPeakSearchMode" isPublic="true" defaultSessionProfile="project://di_signal_analyzer/session_profiles/signal_analyzer_telnet.ffsp">
            <steps>
                <item guid="be3d46c0-cc8e-430b-8ffc-6ad66aa7f05d" action="command" session="$session" normalOffset="61.844" estimatedStepExecutionTime="0.013">
