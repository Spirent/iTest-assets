</description>
                    <defaultValue>30</defaultValue>
                    <isMandatory>true</isMandatory>
                </item>
                <item name="bufferingThreshold">
                    <description>Expressed in Milliseconds (ms)

Default - 0

</description>
                    <defaultValue>0</defaultValue>
                    <isMandatory>true</isMandatory>
                </item>
                <item name="freezingThreshold">
                    <description>Expressed in Milliseconds (ms)

Default - 200
</description>
                    <defaultValue>200</defaultValue>
                    <isMandatory>true</isMandatory>
                </item>
                <item name="enableConcurrentProcessing">
                    <description>Possible values:
true/false

Default - true</description>
                    <defaultValue>true</defaultValue>
                    <isMandatory>true</isMandatory>
                </item>
                <item name="notifyWhenProcessingIsCompleted">
                    <description>Possible values:
true/false

Default - true</description>
                    <defaultValue>true</defaultValue>
                    <isMandatory>true</isMandatory>
                </item>
            </arguments>
            <response>{&quot;resp&quot;:&quot;value&quot;}</response>
