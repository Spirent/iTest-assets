                    <item name="SetDelaySettings" isPublic="true" isJsonPretty="true">
                        <multilineDescription>Updates the current settings of the Delay impairment.</multilineDescription>
                        <arguments>
                            <item name="mapId">
                                <description>“mapId”(required) - The sne map name or id to modify.</description>
                                <isMandatory>true</isMandatory>
                            </item>
                            <item name="impId">
                                <description>“impId” - The impairment name or id.</description>
                                <isMandatory>true</isMandatory>
                            </item>
                            <item name="userToken">
                                <description>“userToken” (optional) - Specifies a valid user account name for multi-user mode.
Not required when multi-user mode is disabled.

Example:
curl -X GET &quot;http://192.168.1.100/api/maps/{mapId}/{port}?userToken={userToken}&quot;
-H &quot;accept: application/json&quot;</description>
                            </item>
                            <item name="delayMode">
                                <description>Sets the type of delay to be updated.

Valid values:

constDelayMode
rangeDelayMode
gaussianDelayMode
rampDelayMode
sinDelayMode</description>
                                <defaultValue>constDelayMode</defaultValue>
                            </item>
                            <item name="enabled">
                                <description>Sets the enabled status of the impairment.

Valid values:

true or false
</description>
                                <defaultValue>false</defaultValue>
                            </item>
                            <item name="maxDataRate">
                                <description>The predicted line rate going through the impairment.

Valid values:


100 
1000
10000
25000</description>
                                <defaultValue>8000</defaultValue>
                            </item>
                            <item name="enableTimeConstraints">
                                <description>Sets the enable status of the time constriants.

Valid values:


true or false
</description>
                                <defaultValue>false</defaultValue>
                            </item>
                            <item name="startDelay">
                                <description>Sets the start delay time in milliseconds.

Valid values:

0.10 to 36000</description>
                                <defaultValue>800</defaultValue>
                            </item>
                            <item name="duration">
                                <description>Sets the run time in milliseconds.

Valid values:

0.10 to 36000</description>
                                <defaultValue>36000</defaultValue>
                            </item>
                            <item name="latency">
                                <description>Sets the latency for the impairment
For 100 and 1000 Data Rate
For 10000, 25000 Data Rate

Valid values:

Dependson maxDataRate:
0.01 to 10000
0.01 to 4000</description>
                                <defaultValue>888</defaultValue>
                            </item>
                            <item name="jitterPcnt">
                                <description>Sets the jitter percent on the impairment.

Valid values:

0 to 100</description>
                                <defaultValue>0</defaultValue>
                            </item>
                            <item name="minLatencyRange">
                                <description>Sets the min value of the latency range.

Valid values:

0.01 to &lt; maxLat </description>
                                <defaultValue>80</defaultValue>
                            </item>
                            <item name="maxLatencyRange">
                                <description>Sets the max value of the latency range.

Valid values:

minLat &lt; to 10000</description>
                                <defaultValue>5089</defaultValue>
                            </item>
                            <item name="allowReorderingRange">
                                <description>Enables/disables reordering.

Valid values:

true or false</description>
                                <defaultValue>false</defaultValue>
                            </item>
                            <item name="minLatencyGauss">
                                <description>Sets the minimum delay value.

Valid values:

0.01 to &lt; maxLat</description>
                                <defaultValue>80</defaultValue>
                            </item>
                            <item name="meanLatency">
                                <description>Sets the max delay value.

Valid values:

minLat &lt; to 10000</description>
                                <defaultValue>800</defaultValue>
                            </item>
                            <item name="deviation">
                                <description>Sets the deviation value.

Valid valules:

0.01 to 10000</description>
                                <defaultValue>8</defaultValue>
                            </item>
                            <item name="allowReorderingGauss">
                                <description>Enables/disables reordering.

Valid values:

true or false</description>
                                <defaultValue>false</defaultValue>
                            </item>
                            <item name="startLatency">
                                <description>Sets the minimum delay value.

Valid values:

0.01 to 30000.00</description>
                                <defaultValue>80</defaultValue>
                            </item>
                            <item name="startPeriod">
                                <description>Sets the max delay value.

Valid values:

0.01 to 10000</description>
                                <defaultValue>800</defaultValue>
                            </item>
                            <item name="endLatency">
                                <description>Sets the deviation value.

Valid values:

0.01 to 10000.00</description>
                                <defaultValue>8000</defaultValue>
                            </item>
                            <item name="riseTime">
                                <description>Sets the time period for the delay to rise.

0.01 to 30000.00</description>
                                <defaultValue>800</defaultValue>
                            </item>
                            <item name="allowReorderingRamp">
                                <description>Enables/disables reordering.

Valid values:

true or false
</description>
                                <defaultValue>false</defaultValue>
                            </item>
                            <item name="minLatencySin">
                                <description>Sets the mini latency of the sin curve.

Valid values:

0.01 to &lt; maxLatency </description>
                                <defaultValue>80</defaultValue>
                            </item>
                            <item name="maxLatencySin">
                                <description>Sets the max latency of the sin curve.

Valid values:

minLat &lt; to 10000</description>
                                <defaultValue>5089</defaultValue>
                            </item>
                            <item name="sinDuration">
                                <description>Sets the sinusoida l duration to which to go between min/max delay.

Valid values:

0.01 to 10000</description>
                                <defaultValue>5</defaultValue>
                            </item>
                            <item name="allowReorderingSin">
                                <description>Enables/disables reordering.

Valid values:

true or false</description>
                                <defaultValue>false</defaultValue>
                            </item>
                        </arguments>
                        <response>{
  &quot;delayMode&quot;: &quot;sinDelayMode&quot;,
  &quot;enabled&quot;: false,
  &quot;maxDataRate&quot;: 8000,
  &quot;timeConstraints&quot;: {
    &quot;enableTimeConstraints&quot;: false,
    &quot;startDelay&quot;: 800,
    &quot;duration&quot;: 36000
  },
  &quot;delayModeSettings&quot;: {
    &quot;constDelayMode&quot;: {
      &quot;latency&quot;: 888,
      &quot;jitterPcnt&quot;: 0
    },
    &quot;rangeDelayMode&quot;: {
      &quot;minLatency&quot;: 80,
      &quot;maxLatency&quot;: 8000,
      &quot;allowReordering&quot;: false
    },
    &quot;gaussianDelayMode&quot;: {
      &quot;minLatency&quot;: 80,
      &quot;meanLatency&quot;: 800,
      &quot;deviation&quot;: 8,
      &quot;allowReordering&quot;: false
    },
    &quot;rampDelayMode&quot;: {
      &quot;startLatency&quot;: 80,
      &quot;startPeriod&quot;: 800,
      &quot;endLatency&quot;: 8000,
      &quot;riseTime&quot;: 800,
      &quot;allowReordering&quot;: false
    },
    &quot;sinDelayMode&quot;: {
      &quot;minLatency&quot;: 80,
      &quot;maxLatency&quot;: 5089,
      &quot;sinDuration&quot;: 5,
      &quot;allowReordering&quot;: false
    }
  }
}</response>
                    </item>
